{
    "name": "vscode-pickdate",
    "description": "VSCode Syntax extension to display internal Pick date.",
    "displayName": "Pickdate!",
    "homepage": "https://github.com/stuboydl/pickdate/blob/master/README.md",
    "icon": "pickdate-icon.png",
    "license": "MIT",
    "publisher": "stuboydl",
    "version": "1.0.2",
    "bugs": {
        "url": "https://github.com/stuboydl/pickdate/issues"
    },
    "engines": {
        "vscode": "^1.70.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:pickdate.showdate"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [{
            "command": "pickdate.showdate",
            "title": "Pick Date"
        }],
        "keybindings": [{
            "command": "pickdate.showdate",
            "key": "ctrl+p,ctrl+p"
        }]
    },
    "galleryBanner": {
        "color": "#0000FF",
        "theme": "dark"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/stuboydl/pickdate"
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/vscode": "^1.70.0",
        "@types/glob": "^7.2.0",
        "@types/mocha": "^9.1.1",
        "@types/node": "16.x",
        "@typescript-eslint/eslint-plugin": "^5.31.0",
        "@typescript-eslint/parser": "^5.31.0",
        "eslint": "^8.20.0",
        "glob": "^8.0.3",
        "mocha": "^10.0.0",
        "typescript": "^4.7.4",
        "@vscode/test-electron": "^2.1.5"
    }
}